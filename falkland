<?xml version="1.0" encoding="UTF-8"?><svg id="Layer_2" xmlns="http://www.w3.org/2000/svg" width=100% height=100% class="state" viewBox="0 0 158.53 121.22"><defs><style>.state{fill:#5a5c5e;stroke:rgba(255,255,255,.25);stroke-miterlimit:10;}</style></defs><g id="NORTH_AMERICA"><g id="US_STATES"><path id="falkland" class="state" d="m1.54,120.72L.5.5h157.53v119.7H44.03l-42.49.52Z"/></g></g>
<script>
  function showInfo(event) {
  const pathId = event.target.getAttribute('id');
  const entry = document.querySelector('.entry[data-country="' + pathId + '"]');
  if (entry) {
    entry.classList.remove('hidden');
  }
}

const paths = document.querySelectorAll('path');
paths.forEach((path) => {
  path.addEventListener('click', showInfo);
});
  </script>
</svg>
